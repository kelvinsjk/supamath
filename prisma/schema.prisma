generator client {
	provider = "prisma-client-js"
}

datasource db {
	provider = "postgresql"
	url      = env("DATABASE_URL")
}

model v2022p1q1 {
	id 	String @id
	a 	Int
	b 	Int
	x1	Int
	y1	Int
	x2	Int
	y2	Int
	x3	Int
	y3	Int
	b2	Int
	og	Boolean @default(false)
	idx	Int @unique
}

model v_inequalities_example {
	id				String @id
	a					Int
	b 				Int
	c 				Int
	d 				Int
	B 				Int
	signCase	Int
	idx				Int @unique
	checked		Boolean @default(false)
	flagged		Boolean @default(false)
}